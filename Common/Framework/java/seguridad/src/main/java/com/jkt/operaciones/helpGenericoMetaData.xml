<?xml version="1.0" ?>
<!DOCTYPE entity [
  <!ELEMENT entidad     	(campos*)>
  <!ELEMENT campos   		(campo*)>
  <!ELEMENT campo 			EMPTY>
  
  <!ATTLIST entidad nombre	CDATA #REQUIRED>

  <!ATTLIST campo fieldName CDATA #REQUIRED>
  <!ATTLIST campo tipo CDATA #REQUIRED>
  <!ATTLIST campo longitud CDATA #REQUIRED>
  <!ATTLIST campo readOnly CDATA #REQUIRED>
  <!ATTLIST campo columnWidth CDATA #IMPLIED>
  <!ATTLIST campo visible CDATA #REQUIRED>
  <!ATTLIST campo orden CDATA #REQUIRED>
  <!ATTLIST campo label CDATA #REQUIRED>

]>

<entidades>

	<entidad nombre="usuario">

		<campos>
			<campo fieldName="oid" tipo="Integer" longitud="0" visible="false" readOnly="true" orden="1" label="oid_usuario" />
			<campo fieldName="Codigo" tipo="String" longitud="15" visible="true" columnWidth="120" readOnly="true" orden="2" label="Codigo" />
			<campo fieldName="Descripcion" tipo="String" longitud="100" visible="true" columnWidth="380" readOnly="true" orden="3" label="Nombre y Apellido" />
		</campos>
		
	</entidad>
		
	<entidad nombre="empresa">

		<campos>
			<campo fieldName="oid" tipo="Integer" longitud="0" visible="false" readOnly="true" orden="1" label="oid_empresa" />
			<campo fieldName="Codigo" tipo="String" longitud="15" visible="true" columnWidth="120" readOnly="true" orden="2" label="Codigo" />
			<campo fieldName="Descripcion" tipo="String" longitud="100" visible="true" columnWidth="380" readOnly="true" orden="3" label="Razon Social" />
		</campos>

	</entidad>

	<entidad nombre="entidad">

		<campos>
			<campo fieldName="oid" tipo="Integer" longitud="0" visible="false" readOnly="false" orden="1" label="oid" />
			<campo fieldName="Codigo" tipo="String" longitud="15" columnWidth="120" visible="true" readOnly="false" orden="2" label="codigo" />
			<campo fieldName="Descripcion" tipo="String" longitud="100" columnWidth="380" visible="true" readOnly="false" orden="3" label="Descripcion" />
		</campos>

	</entidad>
	
</entidades>