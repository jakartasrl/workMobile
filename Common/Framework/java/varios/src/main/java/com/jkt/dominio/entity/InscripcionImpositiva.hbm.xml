<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

	<class name="com.jkt.erp.impuestos.dominio.InscripcionImpositiva" table="INSCRIPCION_IMPOSITIVA" >
		
		<id name="id" column="INSCRIPCION_IMPOSITIVA_ID">
			<generator class="native" />
		</id>

		<property name="activo" column="ACTIVO" type="boolean" />

		<property name="numero" column="NUMERO" />
		<property name="fechaInscripcion" column="FECHA_INSCRIPCION" type="date"></property>

		<many-to-one name="impuesto" class="com.jkt.erp.impuestos.dominio.Impuesto" column="IMPUESTO_ID"></many-to-one>
		<many-to-one name="sujetoImpositivo" class="com.jkt.erp.impuestos.dominio.SujetoImpositivo" column="SUJETO_IMPOSITIVO_ID"></many-to-one>
		
		<list name="categorizacionImpositiva" table="CATEGORIZACION_IMPOSITIVA" lazy="true" cascade="all">
			<key column="CATEGORIZACION_IMPOSITIVA_ID" />
			<index column="ORDEN" />
			<one-to-many class="com.jkt.erp.impuestos.dominio.CategorizacionImpositiva" />
		</list>

		
	</class>

</hibernate-mapping>