<?xml version="1.0" ?>
<!DOCTYPE elementos [
	<!ELEMENT elementos			(elemento*)>
	<!ELEMENT elemento			EMPTY>

	<!ATTLIST elemento	key			CDATA 	#REQUIRED> <!-- key se corresponde con la entidad solicitada. es un alias. -->
	<!ATTLIST elemento	value		CDATA   #REQUIRED> <!-- value es la ruta completa de la clase -->
	<!ATTLIST elemento	validador	CDATA   #IMPLIED>
  
]>

<elementos>
	<elemento key="clasificador" value="com.jkt.varios.dominio.Clasificador" />
	<elemento key="empresa" value="com.jkt.dominio.Empresa" validador="com.jkt.validadores.ValidadorEmpresa" />
	<elemento key="usuario" value="com.jkt.dominio.Usuario" validador="com.jkt.validadores.ValidadorUsuario" />
	<elemento key="condicionPago" value="com.jkt.varios.dominio.CondPago" validador="com.jkt.varios.validadores.ValidadorCondPago" />
	<elemento key="impuesto" value="com.jkt.erp.impuestos.dominio.Impuesto" />
	<elemento key="categoriasImpuesto" value="com.jkt.erp.impuestos.dominio.CategoriaImpuesto" />
	<elemento key="Pais" value="com.jkt.varios.dominio.Pais" />
</elementos>
