<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.jkt.erp.varios.Contacto" table="CONTACTO">
		<id name="id" column="CONTACTO_ID">
			<generator class="native"></generator>
		</id>
		
		<property name="apellido" column="APELLIDO"/>
		<property name="nombre" column="NOMBRE"/>

		<property name="telefono" column="TELEFONO"/>
		<property name="mail" column="MAIL"/>
		<property name="tratamiento" column="TRATAMIENTO"/>

	</class>
	
</hibernate-mapping>