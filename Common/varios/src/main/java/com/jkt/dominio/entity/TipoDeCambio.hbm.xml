<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.jkt.varios.dominio.TipoDeCambio" table="TIPO_CAMBIO">
		<id name="id" column="TIPO_CAMBIO_ID">
			<generator class="native"></generator>
		</id>
		
		<property name="fecha" column="FECHA" type="date"/>
		<many-to-one name="moneda" class="com.jkt.varios.dominio.Moneda" column="MONEDA_ID" cascade="none" />		
		<property name="cotizacion" column="COTIZACION" type="double"/>
		
		<property name="activo" column="ACTIVO" type="boolean" />
				
	</class>
	
</hibernate-mapping>