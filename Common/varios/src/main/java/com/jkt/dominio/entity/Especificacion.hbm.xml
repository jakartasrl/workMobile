<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.jkt.varios.dominio.Especificacion" table="ESPECIFICACION">
		<id name="id" column="ESPECIFICACION_ID">
			<generator class="native"></generator>
		</id>
		
		<property name="ruta" column="RUTA"/>
		<property name="nombre" column="NOMBRE"/>
		<property name="comentario" column="NOTA" />
		<property name="activo" column="ACTIVO" type="boolean" />
		<property name="fechaDeSubida" column="FECHA_SUBIDA_2" />
<!-- 		<property name="fechaDeSubida" column="FECHA_SUBIDA" type="date" /> -->
		<property name="identificadorDeUsuario" column="IDENTIFICADOR_DE_USUARIO" type="integer" />
		
	</class>
	
</hibernate-mapping>