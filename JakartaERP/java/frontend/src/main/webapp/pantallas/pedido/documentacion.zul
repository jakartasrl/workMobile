<zk viewModel="@id('vm') @load(viewModel)">

	<listbox model="@load(vm.lDocumentacion.list)" multiple="true"
		hflex="1" vflex="1" checkmark="true">
		<listhead>
			<listheader width="10%" label="Entregado" />
			<listheader width="5%" label="Codigo" />
			<listheader width="85%" label="Descripcion Documento" />
		</listhead>

		<template name="model">
			<listitem>
				<listcell />
				<listcell label="@load(each.codigo)" />
				<listcell label="@load(each.descripcion)" />
			</listitem>
		</template>

	</listbox>
	
</zk>