<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

	<class name="com.jkt.cotizador.dominio.ConceptoPresupuesto" table="CONCEPTO_PRESUPUESTO" >
		
		<id name="id" column="CONCEPTO_PRESUPUESTO_ID">
			<generator class="native" />
		</id>

		<property name="codigo" type="string">
            <column name="CODIGO" length="15" />
        </property>
		
		<property name="descripcion" column="DESCRIPCION"/>
		
		<property name="activo" column="ACTIVO" type="boolean" />
		<property name="pideArticulo" column="PIDE_ARTICULO" type="boolean" />
		
		<many-to-one name="componenteValor" class="com.jkt.varios.dominio.ComponenteValor" column="COMPONENTE_VALOR_ID"></many-to-one>
		<many-to-one name="titulo" class="com.jkt.cotizador.dominio.TituloModeloCotizador" column="TITULO_MODELO_COTIZADOR_ID"></many-to-one>
		
		<many-to-one lazy="false" name="unidadDeMedidaPorDefecto" class="com.jkt.varios.dominio.UnidadMedida" column="UNIDAD_MEDIDA_ID" cascade="none"></many-to-one>		
		<many-to-one lazy="false" name="monedaPorDefecto" class="com.jkt.varios.dominio.Moneda" column="MONEDA_ID" cascade="none"></many-to-one>		
		
		
	</class>

</hibernate-mapping>