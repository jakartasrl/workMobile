<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.jkt.dominio.ListaPrecioDetalle" table="LISTA_PRECIOS_DETALLE">

		<id name="id" column="LISTA_PRECIOS_DETALLE_ID">
			<generator class="native"></generator>
		</id>
		
		<property name="precio" column="PRECIO" type="double"/>
		
		<many-to-one name="listaPrecios" class="com.jkt.dominio.ListaPrecios" column="LISTA_PRECIOS_ID"></many-to-one>
		
		<many-to-one name="producto" class="com.jkt.erp.articulos.Producto" column="PRODUCTO_ID"></many-to-one>
		<many-to-one name="determinacion" class="com.jkt.laboratorio.dominio.Determinacion" column="DETERMINACION_ID"></many-to-one>

	</class>
	
</hibernate-mapping>