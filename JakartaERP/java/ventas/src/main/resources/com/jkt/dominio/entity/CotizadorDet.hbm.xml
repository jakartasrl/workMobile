<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.jkt.cotizador.dominio.CotizadorDet" table="COTIZADOR_DET">
		<id name="id" column="COTIZADOR_DET_ID">
			<generator class="native"></generator>
		</id>

		<many-to-one name="producto" class="com.jkt.erp.articulos.Producto" cascade="none" column="PRODUCTO_ID"/>

		<property name="cantidad" column="CANTIDAD" type="integer" />
		<property name="precioUnitario" column="PRECIO_UNITARIO" type="double"/>
		<property name="cotizacion" column="COTIZACION" type="double"/>

		<many-to-one name="cotizador" class="com.jkt.cotizador.dominio.Cotizador" column="COTIZADOR_ID" cascade="none" />
		<many-to-one name="conceptoPresupuesto" class="com.jkt.cotizador.dominio.ConceptoPresupuesto" column="CONCEPTO_PRESUPUESTO_ID" cascade="none" />
		<many-to-one name="unidadMedida" class="com.jkt.varios.dominio.UnidadMedida" column="UNIDAD_MEDIDA_ID" cascade="none" />
		<many-to-one name="moneda" class="com.jkt.varios.dominio.Moneda" column="MONEDA_ID" cascade="none" />

		<property name="activo" column="ACTIVO" type="boolean" />
		
	</class>

</hibernate-mapping>

