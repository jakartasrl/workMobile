<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.jkt.pedido.dominio.PedidoDocumentacion" table="PEDIDO_DOCUMENTACION">
		<id name="id" column="PEDIDO_DOCUMENTACION_ID">
			<generator class="native"></generator>
		</id>
		
		<property name="entregado" type="boolean" column="ENTREGADO"></property>
<!-- 		<many-to-one name="pedido" class="com.jkt.pedido.dominio.Pedido" column="PEDIDO_ID" cascade="none" /> -->
		<many-to-one name="documentacion" class="com.jkt.pedido.dominio.Documentacion" column="DOCUMENTACION_ID" cascade="none" />

	</class>
	
</hibernate-mapping>