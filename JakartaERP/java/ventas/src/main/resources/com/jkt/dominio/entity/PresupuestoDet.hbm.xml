<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

	<class name="com.jkt.presupuesto.dominio.PresupuestoDet" table="PRESUPUESTO_DET" >
		
		<id name="id" column="PRESUPUESTO_DET_ID">
			<generator class="native" />
		</id>
			
		<property name="descripcion" type="text" column="DESCRIPCION" />
		
		<many-to-one name="comprobanteVenta" class="com.jkt.dominio.ComprobanteVenta" cascade="none" column="COMPROBANTE_VENTA_ID"/>
		<many-to-one name="presupuesto" class="com.jkt.presupuesto.dominio.Presupuesto" cascade="none" column="PRESUPUEST_ID"/>
		
		<many-to-one name="producto" class="com.jkt.erp.articulos.Producto" cascade="all" column="PRODUCTO_ID"/>
		<many-to-one name="determinacion" class="com.jkt.laboratorio.dominio.Determinacion" cascade="all" column="DETERMINACION_ID"/>
		
		<property name="tipoVenta" column="TIPO" type="integer" />

		<property name="referencia" column="REFERENCIA" type="string" />

		<property name="cantidad" column="CANTIDAD" type="integer" />
		<property name="precio" column="PRECIO" type="double"/>
		
		<property name="activo" column="ACTIVO" type="boolean" />
		
	</class>
	
</hibernate-mapping>