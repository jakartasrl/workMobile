<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

	<class name="com.jkt.erp.articulos.ProductoClasificador" table="PRODUCTO_CLASIFICADOR" >
		
		<id name="id" column="PRODUCTO_CLASIFICADOR_ID">
			<generator class="native" />
		</id>

		<many-to-one name="clasificador" class="com.jkt.varios.dominio.Clasificador" column="CLASIFICADOR_ID"></many-to-one>
		<many-to-one name="componenteValor" class="com.jkt.varios.dominio.ComponenteValor" column="COMPONENTE_VALOR_ID"></many-to-one>
		<many-to-one name="producto" class="com.jkt.erp.articulos.Producto" column="PRODUCTO_ID"></many-to-one>

	</class>

</hibernate-mapping>