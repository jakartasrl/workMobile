<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.jkt.erp.articulos.ArticuloStockDet" table="ARTICULO_STOCK_DET">
		<id name="id" column="ARTICULO_STOCK_DET_ID">
			<generator class="native"></generator>
		</id>

		<property name="valorEntero" column="VALOR_ENTERO" type="integer"/>
		<property name="valorDoble" column="VALOR_DOBLE" type="double"/>
	
		<property name="valorString" column="VALOR_STRING"/>
		<property name="valorBoolean" column="VALOR_BOOLEAN" type="boolean" />

		<many-to-one name="articuloStock" class="com.jkt.erp.articulos.ArticuloStock" column="ARTICULO_STOCK_ID" cascade="none"></many-to-one>
		<many-to-one name="caracProducto" class="com.jkt.erp.articulos.CaracteristicaProducto" column="CARACTERISTICA_PRODUCTO_ID" cascade="none"></many-to-one>
		<many-to-one name="valorTabla" class="com.jkt.erp.articulos.ValoresTablas" column="TABLA_VALORES_ID" cascade="none"></many-to-one>

	</class>

</hibernate-mapping>