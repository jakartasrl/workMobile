<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.jkt.laboratorio.dominio.Determinacion" table="DETERMINACION">
		<id name="id" column="ID_DETERMINACION">
			<generator class="native"></generator>
		</id>

		<property name="codigo" type="string">
			<column name="CODIGO" length="15" />
		</property>

		<property name="descripcion" column="DESCRIPCION" type="string" />
		<property name="activo" column="ACTIVO" type="boolean" />
		<property name="tipoResultado" column="TIPO_RES" type="string" />
		<property name="leyendaValorCero" column="LEYEN_CERO" type="string" />
		<property name="formato" column="FORMATO" type="string" />
		<property name="calculaResultado" column="CALCULA_RES" type="boolean" />

		<many-to-one name="laboratorio" column="ID_LABORATORIO" />

		<list name="metodos" cascade="save-update">
			<key column="ID_DETERMINACION" />
			<list-index column="secu" />
			<one-to-many class="com.jkt.laboratorio.dominio.Metodo" />
		</list>

		<!-- <list name="variables" cascade="save-update"> -->
		<!-- <key column="ID_DETERMINACION" /> -->
		<!-- <list-index column="secu" /> -->
		<!-- <one-to-many class="com.jkt.laboratorio.dominio.Variable" /> -->
		<!-- </list> -->


	</class>

</hibernate-mapping>