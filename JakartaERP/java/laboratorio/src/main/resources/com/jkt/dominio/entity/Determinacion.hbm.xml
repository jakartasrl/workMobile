<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.jkt.laboratorio.dominio.Determinacion" table="DETERMINACION">
		<id name="id" column="ID_DETERMINACION">
			<generator class="native"></generator>
		</id>
		
		<property name="codigo" type="string">
            <column name="CODIGO" length="15" />
        </property>
		
		<property name="descripcion" column="DESCRIPCION" type="string"/>
		<property name="activo" column="ACTIVO" type="boolean" />
		<property name="metodo" column="METODO" type="string"/>
		<property name="tipoResultado" column="TIPO_RES" type="string"/>
		
		<many-to-one name="analisis" lazy="proxy" not-null="true" column="ID_ANALISIS"/>
		
		<list name="listaValoresValidosDeterminacion" inverse="true" cascade="save-update">
			<key column="ID_DETERMINACION" />
			<index column="secu" />
			<one-to-many class="com.jkt.laboratorio.dominio.ValoresValidosDeterminacion" />
		</list>
		
	</class>
	
</hibernate-mapping>