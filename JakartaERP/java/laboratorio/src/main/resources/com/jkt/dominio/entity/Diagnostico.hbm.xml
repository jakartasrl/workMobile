<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.jkt.laboratorio.dominio.Diagnostico" table="DIAGNOSTICO">
		<id name="id" column="ID_DIAGNOSTICO">
			<generator class="native"></generator>
		</id>

		<property name="codigo" type="string" >
			<column name="CODIGO" length="15" />
		</property>

		<property name="descripcion" column="DESCRIPCION" />
		<property name="activo" column="ACTIVO" type="boolean" />
		
		<many-to-one name="laboratorio" lazy="proxy" not-null="true" column="ID_LABORATORIO"/>
		
		

	</class>

</hibernate-mapping>