<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.jkt.laboratorio.dominio.ProtocoloVariable" table="PROTOCOLO_VARIABLE">
		<id name="id" column="PROTOCOLO_VARIABLE_ID">
			<generator class="native"></generator>
		</id>

		<property name="variable" column="VARIABLE" />
		<property name="descVariable" column="DESC_VARIABLE" />
		<property name="expresion" column="EXPRESION" />
		<property name="valor" column="VALOR" />
		
		<property name="activo" column="ACTIVO" type="boolean" />
		
		<property name="input" column="INPUT" type="boolean" />
		
		<property name="resultadoExpresion" column="RESULTADO_EXPRESION" />
		
		<property name="resultadoFinal" column="RESULTADO_FINAL" type="boolean" />
		
	</class>

</hibernate-mapping>