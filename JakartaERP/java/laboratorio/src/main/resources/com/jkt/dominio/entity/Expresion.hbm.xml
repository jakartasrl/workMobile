<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.jkt.laboratorio.dominio.Expresion" table="ExpresionLab">
		
		<id name="id" column  ="ID_VARIABLE" >
			<generator class="foreign">
				<param name="property">variable</param>
			</generator>
        </id>
        
        
<!--         <one-to-one name="metodo" class="com.jkt.laboratorio.dominio.Metodo" constrained="true"></one-to-one> -->
        <one-to-one name="variable" class="com.jkt.laboratorio.dominio.Variable" constrained="true" />
			
		<property name="expresion" column="EXPRESION" />
	
	</class>

</hibernate-mapping>