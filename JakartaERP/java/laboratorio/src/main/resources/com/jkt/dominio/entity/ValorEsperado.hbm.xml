<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.jkt.laboratorio.dominio.ValorEsperado" table="DETER_VALOR_ESP">
		<id name="id" column="ID_VALOR_ESPE">
			<generator class="native"></generator>
		</id>

		<property name="valorDesde" column="VALOR_DESDE" type="double" />
		<property name="valorHasta" column="VALOR_HASTA" type="double" />
		<property name="limite" column="LIMITE" type="string" />

		<many-to-one name="metodo" column="ID_METODO" />
		<many-to-one name="caracteristica" column="CARACTERISTICA_PRODUCTO_ID" />

	</class>

</hibernate-mapping>