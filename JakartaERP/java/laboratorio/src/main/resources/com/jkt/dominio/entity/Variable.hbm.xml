<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.jkt.laboratorio.dominio.Variable" table="DETER_VARIABLE">
		<id name="id" column="ID_VARIABLE">
			<generator class="native"></generator>
		</id>
		<property name="codigo" type="string">
			<column name="CODIGO" length="15" />
		</property>

		<property name="descripcion" column="DESCRIPCION" type="string" />
		<property name="expresion" column="EXPRESION" type="string" />
		<property name="activo" column="ACTIVO" type="boolean" />
		<property name="input" column="INPUT" type="boolean" />
		<property name="resultadoFinal" column="RESULTADO_FINAL" type="boolean" />

<!-- 		<many-to-one name="determinacion" column="ID_DETERMINACION" /> -->

<!-- 		<many-to-one name="expresion" cascade="all" class="com.jkt.laboratorio.dominio.Expresion"></many-to-one> -->
		
<!-- 		<one-to-one name="expresion" cascade="save-update" class="com.jkt.laboratorio.dominio.Expresion" /> -->

	</class>

</hibernate-mapping>