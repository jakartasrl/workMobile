<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.jkt.laboratorio.dominio.Protocolo" table="PROTOCOLO">
		<id name="id" column="PROTOCOLO_ID">
			<generator class="native"></generator>
		</id>
		
		<many-to-one name="tipoComprobante" class="com.jkt.dominio.TipoComprobante" column="TIPO_COMPROBANTE_ID"></many-to-one>
		<many-to-one name="clienteSucursal" class="com.jkt.erp.varios.ClienteSucursal" column="CLIENTE_SUCURSAL_ID"></many-to-one>

		<property name="nro" column="NUMERO" type="string" />

		<property name="enviado" column="ENVIADO" type="boolean" />
		<property name="impreso" column="IMPRESO" type="boolean" />
		
		<many-to-one name="laboratorio" class="com.jkt.laboratorio.dominio.Laboratorio" column="LABORATORIO_ID"/>
		<many-to-one name="equipo" class="com.jkt.laboratorio.dominio.Equipo" column="EQUIPO_ID"/>
		
		<property name="idPedido" column="ID_PEDIDO" />
		<property name="estado" column="ESTADO" />

		<property name="comentario" column="COMENTARIO" />
		<property name="comentarioDiagnostico" column="COMENTARIO_DIAGNOSTICO" />
		
		<property name="ordenTrabajo" column="ORDEN_TRABAJO" />

		<many-to-one name="usuarioIngresoResultado" 	class="com.jkt.dominio.Usuario" column="USUARIO_RESULTADO_ID"/>
		<many-to-one name="usuarioIngresoAprobacion" 	class="com.jkt.dominio.Usuario" column="USUARIO_APROBACION_ID"/>
		<many-to-one name="usuarioFinalizado" 			class="com.jkt.dominio.Usuario" column="USUARIO_FINALIZACION_ID"/>

		<many-to-one name="diagnostico" class="com.jkt.laboratorio.dominio.Diagnostico" column="DIAGNOSTICO_ID"/>

		<property name="activo" column="ACTIVO" type="boolean" />

		<property name="fecha" 					type="date" column="FECHA_EDICION"></property>
		<property name="fechaHoraMuestra" 		type="date" column="FECHA_HORA_MUESTRA"></property>
		<property name="fechaHoraResultado" 	type="date" column="FECHA_HORA_RESULTADO"></property>
		<property name="fechaHoraAprobacion" 	type="date" column="FECHA_HORA_APROBACION"></property>
		<property name="fechaHoraFinalizacion" 	type="date" column="FECHA_HORA_FINALIZACION"></property>
		
		<list name="detalles" table="PROTOCOLO_DETALLE" cascade="all">
			<key column="PROTOCOLO_ID" />
			<index column="ORDEN" />
			<one-to-many class="com.jkt.laboratorio.dominio.ProtocoloDetalle" />
		</list>


	</class>

</hibernate-mapping>