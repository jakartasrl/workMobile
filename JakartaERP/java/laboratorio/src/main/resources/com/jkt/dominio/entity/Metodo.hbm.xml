<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.jkt.laboratorio.dominio.Metodo" table="DETER_METODO">
		<id name="id" column="ID_METODO">
			<generator class="native"></generator>
		</id>

		<property name="metodo" column="metodo" type="string" />

<!-- 		<one-to-one name="expresion" cascade="save-update" class="com.jkt.laboratorio.dominio.Expresion" /> -->

		<many-to-one name="determinacion" column="ID_DETERMINACION" />

		<list name="valoresEsperados" cascade="save-update">
			<key column="ID_METODO" />
			<list-index column="secu" />
			<one-to-many class="com.jkt.laboratorio.dominio.ValorEsperado" />
		</list>

		<list name="variables"  cascade="save-update">
			<key column="ID_METODO" />
			<list-index column="secu" />
			<one-to-many class="com.jkt.laboratorio.dominio.Variable" />
		</list>

	</class>

</hibernate-mapping>