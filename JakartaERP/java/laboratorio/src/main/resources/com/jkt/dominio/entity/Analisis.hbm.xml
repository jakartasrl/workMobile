<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.jkt.laboratorio.dominio.Analisis" table="ANALISIS">
		<id name="id" column="ID_ANALISIS">
			<generator class="native"></generator>
		</id>

		<property name="codigo" type="string" >
			<column name="CODIGO" length="15" />
		</property>

		<property name="descripcion" column="DESCRIPCION" />
		<property name="activo" column="ACTIVO" type="boolean" />
		
		<many-to-one name="laboratorio" column="ID_LABORATORIO"/>
		
		<list name="listaDeterminaciones"  cascade="save-update">
			<key column="ID_ANALISIS" />
			<index column="secu"/>
			<one-to-many class="com.jkt.laboratorio.dominio.AnalisisDet"/>
		</list>

	</class>

</hibernate-mapping>