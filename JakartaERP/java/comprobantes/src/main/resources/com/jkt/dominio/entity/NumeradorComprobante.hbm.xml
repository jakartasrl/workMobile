<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

	<class name="com.jkt.dominio.NumeradorComprobantes" table="NUMERADOR_COMPROBANTES">
		
		<id name="id" column="NUMERADOR_COMPROBANTES_ID">
			<generator class="native" />
		</id>
		
		<property name="argumento" column="ARGUMENTO" />
		<property name="numero" column="NUMERO" type="integer"/>
		
		<property name="activo" column="ACTIVO" type="boolean" />

	</class>

</hibernate-mapping>